  "scripts": {
    "migrate:all:schemas": "node ./database/scripts/migrate_schemas.js",
    "db:migrate:admin1": "node ./test/sequelize/scripts/migrate.js --migrations_path=./database/migrations/db_administrator_schema",
    "db:migrate:client1": "node ./test/sequelize/scripts/migrate.js --migrations_path=./database/migrations/db_clients_schema",
    "db:migrate:admin": "NODE_ENV=test PG_SCHEMA=db_administrator ./node_modules/.bin/sequelize db:migrate --migrations-path ./database/migrations/db_administrator_schema",
    "db:migrate:client": "NODE_ENV=test PG_SCHEMA=db_client ./node_modules/.bin/sequelize db:migrate --migrations-path ./database/migrations/db_clients_schema",
    "db:migrate": "npm run db:migrate:admin && npm run db:migrate:client",
    "db:migrate:status": "node ./test/sequelize/scripts/migrate-status.js",
    "migrate:status": "PG_SCHEMA=db_sksolutions sequelize db:migrate:status --migrations-path ./database/migrations/db_clients_schema",
    "migrate": "PG_SCHEMA=db_sksolutions sequelize db:migrate --migrations-path ./database/migrations/db_clients_schema",
    "db:migrate:client:create": "sequelize migration:create --migrations-path ./database/migrations/db_clients_schema --name $1",
    "migrate:undo": "PG_SCHEMA=db_sksolutions sequelize db:migrate:undo --migrations-path ./database/migrations/db_clients_schema",
    "migrate:seed": "PG_SCHEMA=db_sksolutions sequelize db:seed:all --seeders-path=./database/seeders/prod/db_clients_schema",
    "migrateAdmin:status": "PG_SCHEMA=db_administrator sequelize db:migrate:status --migrations-path ./database/migrations/db_administrator_schema",
    "migrateAdmin": "PG_SCHEMA=db_administrator sequelize db:migrate --migrations-path ./database/migrations/db_administrator_schema",
    "migrateAdmin:undo": "PG_SCHEMA=db_administrator sequelize db:migrate:undo --migrations-path ./database/migrations/db_administrator_schema",
    "migrateAdmin:seed": "PG_SCHEMA=db_administrator sequelize db:seed:all --seeders-path=./database/seeders/prod/db_administrator_schema",
    "db:drop": "node ./test/sequelize/scripts/drop.js",
    "db:create": "node ./test/sequelize/scripts/create.js",
    "firestore:create": "node ./test/firestore/scripts/createVikkon.js",
    "firestore:createGps": "node ./test/firestore/scripts/devicesGps.js",
    "firestore:clear": "node ./test/firestore/scripts/clearVikkon.js",
    "pretest-sequelize": "NODE_ENV=test npm run db:drop && NODE_ENV=test npm run db:create && NODE_ENV=test npm run db:migrate",
    "test": "mocha test/ --reporter mochawesome",
    "test-seq": "NODE_ENV=test npm run pretest-sequelize && NODE_ENV=test mocha test/sequelize/integration --reporter mochawesome",
    "testnyan": "mocha test/ --reporter nyan",
    "testlist": "mocha test/ --reporter list",
    "testprogress": "mocha test/ --reporter progress",
    "dev": "nodemon server.js",
    "lint": "eslint ./"
  },
  "author": "Makech.io",
